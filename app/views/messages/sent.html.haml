%h1 Messages / Sent

= render 'navigation'

%table
  %thead
    %th Unread
    %th Subject
    %th
  %tbody
    - @messages.each do |message|
      %tr
        %td= #message.read_at?
        %td= link_to message.subject, message_path(message)
        %td= link_to "remove", message_path(message), method: :delete